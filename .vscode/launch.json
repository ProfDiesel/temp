{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python : Fichier actuel",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "ppf",
            "type": "python",
            "request": "launch",
            "module": "ppf"
        },
        {
            "name": "integration python",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/test/integration/main.py",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "integration native",
            "type": "cppdbg",
            "request": "launch",
            "program": "/usr/bin/python3",
            "args": ["${workspaceFolder}/test/integration/main.py"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/test/integration",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Activer l'impression en mode Pretty pour gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}